<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fauzi.hilmy.quizgojek">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/main"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityKuliner"
            android:label="@string/title_kuliner"/>
        <activity android:name=".ActivityHotel"
            android:label="@string/title_hotel"/>
        <activity android:name=".ActivityWisata"
            android:label="@string/title_wisata">

        </activity>
        <activity android:name=".ActivityDetailKuliner"
            android:label="@string/title_kuliner_detail"></activity>
        <activity android:name=".ActivityDetailHotel"
            android:label="@string/title_hotel_detail"></activity>
        <activity android:name=".ActivityDetailWisata"
            android:label="@string/title_wisata_detail"></activity>
    </application>

</manifest>